{
    "builds": [
        {
        "src": "GestionAcademia/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config":{
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "GestionAcademia/wsgi.py"
        }
    ]
}